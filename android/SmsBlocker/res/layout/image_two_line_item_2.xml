<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/vw1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal"
    android:background="@drawable/list_item_bg"
    >    
   <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        >

		<ImageView android:id="@+id/image_two_line_item_2_img1"
        	   android:layout_width="32dip"
	           android:layout_height="32dip"
	           />

		<ImageView android:id="@+id/image_two_line_item_2_img2"
        	   android:layout_width="32dip"
	           android:layout_height="32dip"
	           android:src="@drawable/sym_call_incoming"
	           />
       
        <LinearLayout
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:orientation="vertical"
        	android:layout_marginLeft="5dip"
			> 
        		
	        <TextView android:id="@+id/image_two_line_item_2_text_1"
	            android:textStyle="bold"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:textColor="@color/white"
	    		android:ellipsize="end"
	    		/>
	    		
	    	<TextView android:id="@+id/image_two_line_item_2_text_2"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:textColor="@color/white"
    			android:ellipsize="end"
    			/>
    	</LinearLayout>
	    
    </LinearLayout>
</LinearLayout>